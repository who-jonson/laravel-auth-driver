{
    "name": "who-jonson/laravel-auth-driver",
    "description": "A Custom Authentication Driver & Provider for Laravel 6 or greater.",
    "keywords": ["laravel", "driver", "auth", "file", "provider", "guard"],
    "homepage": "https://github.com/who-jonson/laravel-auth-driver",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonson B.",
            "email": "www.jbc.bd@gmail.com",
            "homepage": "https://who-jonson.github.io",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/who-jonson/laravel-auth-driver/issues",
        "source": "https://github.com/who-jonson/laravel-auth-driver"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "composer/composer": "^1.6 || ^2.0",
        "illuminate/config": "^6.0 || ^7.0 || ^8.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "illuminate/filesystem": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "phpdocumentor/type-resolver": "^1.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.5 || ^9.0"
    },
    "suggest": {
        "laravel/framework": "To test the Laravel bindings",
        "who-jonson/laravel-organizer": "To organize the Laravel project"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WhoJonson\\LaravelAuth\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "WhoJonson\\LaravelAuth\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WhoJonson\\Tests\\LaravelAuth\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
